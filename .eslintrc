{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-param-reassign": 0,
    "no-console": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-bitwise": 0,
    "max-len": 0,
    "no-continue": 0,
    "no-cond-assign": 0
  }
}
