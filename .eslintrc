{
  "extends": "airbnb-base",
  "env": {
    "browser":  true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-param-reassign": 0,
    "no-console": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-bitwise": 0,
    "max-len": 0,
    "no-continue": 0,
    "no-cond-assign": 0,
    "no-plusplus": 0
  }
}
