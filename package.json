    "@ledgerhq/hw-transport-u2f": "^4.57.0",
    "bignumber.js": "^8.0.1",
    "bip39": "^3.0.0",
    "blake2b": "^2.1.3",
    "bs58check": "^2.1.2",
    "buffer": "^5.2.1",
    "detect-node": "^2.0.4",
    "libsodium-wrappers": "^0.7.3",
    "pbkdf2": "^3.0.16",
    "secp256k1": "^3.7.0",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "@types/bip39": "^2.4.2",
    "@types/detect-node": "^2.0.0",
    "@types/ledgerhq__hw-transport": "^4.21.1",
